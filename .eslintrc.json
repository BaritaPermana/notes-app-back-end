{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "eol-last": [
            "error",
            "never"
        ],
        "indent": [
            "error",
            4
        ],
        "no-console": "off"
    }
}